<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: linear-gradient(  #726c93,#2d2c30);" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.network5.NewChatController">
   <children>
      <TableView fx:id="tableViewUsers" layoutX="30.0" layoutY="24.0" onMouseClicked="#handleAdd" prefHeight="305.0" prefWidth="230.0">
         <columns>
            <TableColumn fx:id="tableColumnFirstName" prefWidth="120.79998779296875" text="FirstName" />
            <TableColumn fx:id="tableColumnLastName" prefWidth="108.8000244140625" text="LastName" />
         </columns>
      </TableView>
      <Button layoutX="208.0" layoutY="348.0" mnemonicParsing="false" onAction="#handleDone" prefHeight="26.0" prefWidth="52.0" text="Send" />
      <Button fx:id="buttonAdd" layoutX="292.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleAddUser" text="add to chat" />
      <TextField fx:id="newMess" layoutX="33.0" layoutY="348.0" prefHeight="26.0" prefWidth="167.0" />
   </children>
</AnchorPane>
