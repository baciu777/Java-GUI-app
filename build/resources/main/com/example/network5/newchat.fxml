   <children>
      <TableView fx:id="tableViewUsers" layoutX="30.0" layoutY="59.0" onMouseClicked="#handleAdd" prefHeight="288.0" prefWidth="212.0">
         <columns>
            <TableColumn fx:id="tableColumnFirstName" prefWidth="120.79998779296875" text="FirstName" />
            <TableColumn fx:id="tableColumnLastName" prefWidth="89.60002441406249" text="LastName" />
         </columns>
      </TableView>
      <Button layoutX="191.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleDone" prefHeight="26.0" prefWidth="52.0" text="Send" />
      <Button fx:id="buttonAdd" layoutX="292.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleAddUser" text="add to chat" />
      <TextField fx:id="newMess" layoutX="30.0" layoutY="361.0" />
   </children>
</AnchorPane>
